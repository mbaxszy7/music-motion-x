{
  "name": "music-motion-x",
  "version": "1.0.7",
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev:client": "SSR=true NODE_ENV=development TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode=development --watch --profile --json=compilation-stats.json",
    "dev:server": "BUILD=server SSR=true NODE_ENV=development TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode=development --config webpack.server.ts --watch ",
    "dev:start": "nodemon ./public/server.app.js",
    "dev:csr": " SSR=false NODE_ENV=development TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack serve --mode=development",
    "build:client:stats": "SSR=true NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode=production --profile --json=compilation-stats.json",
    "build:client": "SSR=true NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode=production",
    "build:server": "BUILD=server SSR=true NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode=production --config webpack.server.ts ",
    "build": "npm run build:client:stats && npm run build:client && npm run build:server",
    "build:start": "node ./public/server.app.js",
    "deploy": "pm2 deploy ecosystem.config.js production",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write src/",
      "eslint --fix --max-warnings 0"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "browserslist": {
    "production": [
      "> 0.25% and not dead"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ],
    "ssr": [
      "node 14"
    ]
  },
